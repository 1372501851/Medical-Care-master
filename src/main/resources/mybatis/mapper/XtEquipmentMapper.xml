            <if test="uequipmentTesting != null and uequipmentTesting != ''">uequipment_testing = #{uequipmentTesting},</if>
            <if test="ubrownOut != null and ubrownOut != ''">ubrown_out = #{ubrownOut},</if>
            <if test="unetworkError != null and unetworkError != ''">unetwork_error = #{unetworkError},</if>
            <if test="uploadDownload != null and uploadDownload != ''">upload_download = #{uploadDownload},</if>
            <if test="drippingSpeedMin != null">dripping_speed = #{drippingSpeedMin},</if>
            <if test="drippingSpeedMax != null">dripping_speed = #{drippingSpeedMax},</if>
            <if test="uequipmentPwd != null">uequipment_pwd = #{uequipmentPwd},</if>
            <if test="ucreatedate != null">ucreatedate = #{ucreatedate},</if>
        </trim>
        where uequipmentid = #{uequipmentid}
    </update>

    <delete id="deleteXtEquipmentByUequipmentid" parameterType="String">
        delete from xt_equipment where uequipmentid = #{uequipmentid}
    </delete>

    <delete id="deleteXtEquipmentByUequipmentids" parameterType="String">
        delete from xt_equipment where uequipmentid in
        <foreach item="uequipmentid" collection="array" open="(" separator="," close=")">
            #{uequipmentid}
        </foreach>
    </delete>
</mapper>
