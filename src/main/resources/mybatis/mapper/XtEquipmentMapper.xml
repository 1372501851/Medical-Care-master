<mapper namespace="com.ruoyi.project.merchant.mapper.XtEquipmentMapper">

    <resultMap type="XtEquipment" id="XtEquipmentResult">
        <result property="uequipmentid"    column="uequipmentid"    />
        <result property="uequipmentTypeid"    column="uequipment_typeid"    />
        <result property="uequipmentname"    column="uequipmentname"    />
        <result property="uqrCode"    column="uqr_code"    />
        <result property="uquipmentCode"    column="uquipment_code"    />
        <result property="userid"    column="userid"    />
        <result property="ucompid"    column="ucompid"    />
        <result property="udepartmentid"    column="udepartmentid"    />
        <result property="uareaid"    column="uareaid"    />
        <result property="ulatitude"    column="ulatitude"    />
        <result property="ulongitude"    column="ulongitude"    />
        <result property="uaddress"    column="uaddress"    />
        <result property="uequipmentStatus"    column="uequipment_status"    />
        <result property="uequipmentError"    column="uequipment_error"    />
        <result property="uequipmentTesting"    column="uequipment_testing"    />
        <result property="ubrownOut"    column="ubrown_out"    />
        <result property="unetworkError"    column="unetwork_error"    />
        <result property="uploadDownload"    column="upload_download"    />
        <result property="drippingSpeedMin"    column="dripping_speed_min"    />
