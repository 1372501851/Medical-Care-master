    <sql id="selectXtProductVo">
        select uproductid, uproducttypeid, ucompid, ucustomerid, uproduct_name, uproduct_form, uproduct_pic, ubarcode, ubarcode_numeric_code, ucategory_name, usort_name, utrade_name, uproduct_model, uproduct_specs, uproduct_dose, umanufacturer, umaterial_code, uproduct_standards, uapproval_no, ucrtificate_inspection, ucertificate, umain_components, umain_efficacy, usage_method, usingle_treatment, umatters_attention, ustorage_method, ushelf_life, uexpiration_date, uinventory_quantity, udelivery_quantity, uReceipt_quantity, utransaction_price, uretail_price, umembership_price, uprice_huicheng, uwholesale_price, uagency_price, uoutsourcing_price, upurchase_price, ucommodity_losses, uremarks1, uremarks2, flag, status, create_time, update_time from xt_product
    </sql>

    <select id="selectXtProductList" parameterType="XtProduct" resultMap="XtProductResult">
        select uproductid, uproducttypeid, xt_product.ucompid, xt_product.ucustomerid, uproduct_name, uproduct_form, uproduct_pic, ubarcode, ubarcode_numeric_code, ucategory_name, usort_name, utrade_name, uproduct_model, uproduct_specs, uproduct_dose, umanufacturer, umaterial_code, uproduct_standards, uapproval_no, ucrtificate_inspection, ucertificate, umain_components, umain_efficacy, usage_method, usingle_treatment, umatters_attention, ustorage_method, ushelf_life, uexpiration_date, uinventory_quantity, udelivery_quantity, uReceipt_quantity, utransaction_price, uretail_price, umembership_price, uprice_huicheng, uwholesale_price, uagency_price, uoutsourcing_price, upurchase_price, ucommodity_losses, uremarks1, uremarks2, flag, status, xt_product.create_time, xt_product.update_time,ucompname
        from xt_product left join xt_comp on xt_product.ucompid = xt_comp.ucompid
        where flag = '0'
        <if test="ucompid != null ">
            AND xt_product.ucompid = #{ucompid}
        </if>
        <if test="uproducttypeid != null ">
            AND uproducttypeid = #{uproducttypeid}
        </if>
        <if test="uproductName != null ">
            AND uproduct_name = #{uproductName}
        </if>
        <if test="uproductForm != null ">
            AND uproduct_form = #{uproductForm}
        </if>
        <if test="uproductid != null ">
            AND uproductid = #{uproductid}
        </if>
<!--        <if test="sort == 1 and SalesQuantity != null">-->
<!--            order by  SalesQuantity-->
<!--        </if>-->
<!--        <if test="sort == 2 ">-->
<!--            order by  uretail_price-->
<!--        </if>-->





    </select>

    <select id="selectXtProductByUproductid" parameterType="String" resultMap="XtProductResult">
        <include refid="selectXtProductVo"/>
        where uproductid = #{uproductid} and flag = '0'
    </select>
    <select id="selectXtProductByUpCompId" parameterType="String" resultMap="XtProductResult">
        <include refid="selectXtProductVo"/>
        where ucompid = #{compId} and flag = '0'
    </select>
    <select id="selectProductTypesBycompId" resultType="java.lang.String">
        select distinct(uproducttypeid)
        from xt_product
        where ucompid = #{ucompid}
    </select>

