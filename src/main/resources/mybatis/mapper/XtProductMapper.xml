    <update id="banXtProductByUproductids">
        update xt_product
        set status = '2'
        where uproductid in
        <foreach item="uproductid" collection="array" open="(" separator="," close=")">
            #{uproductid}
        </foreach>
    </update>

    <update id="releaseXtProductByUproductids">
        update xt_product
        set status = '0'
        where uproductid in
        <foreach item="uproductid" collection="array" open="(" separator="," close=")">
            #{uproductid}
        </foreach>
    </update>

    <delete id="deleteXtProductByUproductid" parameterType="String">
        delete from xt_product where uproductid = #{uproductid}
    </delete>


    <delete id="deleteXtProductByUproductids" parameterType="String">
        update xt_product
        set flag = '2'
        where uproductid in
        <foreach item="uproductid" collection="array" open="(" separator="," close=")">
            #{uproductid}
        </foreach>
    </delete>

    <select id="selectProductByPlAndZl" resultType="XtProduct" parameterType="string">
        select * from xt_product
        <where>
            <if test="sortName != null  and sortName != ''"> and usort_name = #{sortName}</if>
            <if test="categoryName != null  and categoryName != ''"> and ucategory_name = #{categoryName}</if>
        </where>

    </select>

